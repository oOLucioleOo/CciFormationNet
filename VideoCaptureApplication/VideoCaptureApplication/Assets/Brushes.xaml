<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- ***** Colors ***** -->
    <Color x:Key="LightGrayColor">#BDBBC6</Color>
    <Color x:Key="DarkGrayColor">#FF414141</Color>
    <Color x:Key="RedColor">#AC0040</Color>
    <Color x:Key="BlueColor">#3366b6</Color>
    <Color x:Key="DarkBlueColor">#164186</Color>

    <!-- ***** Brushes ***** -->
    <SolidColorBrush x:Key="LightGrayBrush" Color="{StaticResource LightGrayColor}"/>
    <SolidColorBrush x:Key="DarkGrayBrush" Color="{StaticResource DarkGrayColor}"/>
    <SolidColorBrush x:Key="RedBrush" Color="{StaticResource RedColor}" />
    <SolidColorBrush x:Key="BlueBrush" Color="{StaticResource BlueColor}" />
    <SolidColorBrush x:Key="DarkBlueBrush" Color="{StaticResource DarkBlueColor}" />


</ResourceDictionary>